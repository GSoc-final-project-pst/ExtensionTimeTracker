{
    "manifest_version": 3,
    "name": "Simple Time Tracker",
    "version": "1.0",
    "description": "Tracks time spent on websites.",
    "permissions": ["storage", "tabs"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
      }
 }
  
  